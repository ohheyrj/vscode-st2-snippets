{
    "St2 Pack Config File Template": {
        "prefix": "!st2-config",
        "body": [
            "---",
            "ref: ${1:PackReference}",
            "name: ${2:PackName}",
            "description: ${3:PackDescription}.",
            "keywords:",
            "    - ${4:Keyword1}",
            "    - ${5:Keyword2}",
            "version: 0.1.0",
            "author: ${6:Author}",
            "email: ${7:Email}"
        ],
        "desription": "Template pack.yaml file"},
    "St2 Configuration Scheme Template Entry": {
        "prefix": "!st2-cfgsch",
        "body": [
          "  ${1:ConfigItemName}:",
          "    description: ${2:Description}",
          "    type: ${3:ConfigType}",
          "    required: ${4:ConfigRequired}",
          "    secret: ${5:ConfigSecret}"
        ],
        "description": "Entry for config schema"
    }
}